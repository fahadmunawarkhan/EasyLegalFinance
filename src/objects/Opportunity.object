&amp;IF(ISBLANK(APR_1__c),&quot;\\nAPR 1&quot;,&quot;&quot;)
&amp;IF(ISBLANK(APR_2__c),&quot;\\nAPR 2&quot;,&quot;&quot;)
&amp;IF(ISBLANK(Style_of_Cause_Description__c),&quot;\\nStyle of Cause Description&quot;,&quot;&quot;)
