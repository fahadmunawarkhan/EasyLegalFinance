    <validationRules>
        <fullName>Cannot_Change_Key_Fields_after_Paid_Out</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Count_of_Payments__c&gt;0,
     OR(
        IsChanged(Interest_Rate__c),
        IsChanged( Compounding_Interest__c ),
        IsChanged(  Interest_Compounding_Period__c  ),
        IsChanged( Interest_Deferral_Period__c ),
        IsChanged( Minimum_Interest_Period__c )
     )
 )</errorConditionFormula>
        <errorMessage>Cannot change loan setup information after Payments created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Have_Min_interest_and_Defferred</fullName>
        <active>false</active>
        <errorConditionFormula>AND(NOT(IsPickVal(Minimum_Interest_Period__c,&quot;&quot;)),
    NOT(OR(IsPickVal(Interest_Deferral_Period__c,&quot;0&quot;),
           IsPickVal(Interest_Deferral_Period__c,&quot;&quot;))))</errorConditionFormula>
        <errorMessage>Cannot have both minimum interest and deferred interest.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Compounding_Interest_Requires_Frequency</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ispickval(Interest_Compounding_Period__c,&apos;Compounding Interest&apos;), ispickval(Compounding_Interest__c,&quot;&quot;))</errorConditionFormula>
        <errorMessage>As Compounding Interest is selected please provide a value for Compounding Frequency.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPP_VR_01_Assessment</fullName>
        <active>true</active>
        <description>Facility Loans are the only opportunities that can be re-assessed</description>
        <errorConditionFormula>AND(
NOT(ISPICKVAL(Type_of_Loan__c, &quot;Facility Loan&quot;)),
ISPICKVAL(StageName, &quot;Re-Assessment&quot;)
)</errorConditionFormula>
        <errorMessage>The Re-Assessment Stage is not available for Opportunities that are not Facility Loans</errorMessage>
    </validationRules>
