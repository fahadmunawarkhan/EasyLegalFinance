        <description>Can only trigger Payment allocation on Payment type Drowdowns and when outstanding balance is less than zero.</description>
        <errorConditionFormula>AND ( Allocate_Payment__c,
      OR( Outstanding_Balance_as_of_Payout_Date__c &gt;0 ,
          NOT(ispickval( Payment_Method__c , &quot;Payment&quot;))))</errorConditionFormula>
        <errorDisplayField>Allocate_Payment__c</errorDisplayField>
        <errorMessage>Can only trigger Payment allocation on Payment type Drowdowns and when outstanding balance is less than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Change_Key_Fields_after_Paid_Out</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT(ISBLANK(Date_of_Last_Repayment__c )),
     OR(
        IsChanged(Amount__c),
        IsChanged(Date__c),
		IsPickval(Payment_Method__c,&quot;Payment&quot;),
		IsPickval(Reference_Notes__c,&quot;Payment Rejected&quot;),
		IsPickval(Reference_Notes__c,&quot;Payment Re-processed&quot;)))</errorConditionFormula>
        <errorMessage>Cannot change the Amount, Date, Payment Method, or Reference Notes after payments are allocated.  Remove payment first.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_create_drawdown_wo_loan_set_up</fullName>
        <active>true</active>
        <description>Prevent users from creating drawdown records before the loan set up section in the opportunity is populated.</description>
        <errorConditionFormula>AND( 
    ISNEW(), 
    OR( 
        AND(
            ISPICKVAL(Opportunity__r.Minimum_Interest_Period__c, &apos;&apos;), 
            NOT(OR(IsPickVal(Opportunity__r.Interest_Deferral_Period__c,&apos;0&apos;), 
                   IsPickVal(Opportunity__r.Interest_Deferral_Period__c,&apos;&apos;)))
            ),
        ISBLANK( Opportunity__r.Interest_Rate__c), 
		ISPICKVAL(Opportunity__r.Interest_Compounding_Period__c,&apos;&apos;),
		AND(
            ISPICKVAL(Opportunity__r.Compounding_Interest__c, &apos;&apos;),
			ISPICKVAL(Opportunity__r.Interest_Compounding_Period__c,&apos;Compounding Interest&apos;)
           )
		) 
    )</errorConditionFormula>
        <errorMessage>You cannot create drawdowns before populating the loan set up.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Payment_Rejected_Needs_to_be_Negative</fullName>
        <active>true</active>
        <description>Payment Rejected needs a negative amount, please fix and try again.</description>
        <errorConditionFormula>AND(
ISPICKVAL(Reference_Notes__c, &apos;Payment Rejected&apos;),
Amount__c &gt; 0
)</errorConditionFormula>
        <errorMessage>Payment Rejected needs a negative amount, please fix and try again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPD_Total_Balance_Check</fullName>
        <active>true</active>
        <description>Total Drawdowns cannot be greater than Service Provider allowance</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Opportunity_Service_Provider__c)),
ISCHANGED(Amount__c),
Amount__c &gt; Service_Provider_Allowed_Balance__c,
NOT( ISPICKVAL( Payment_Method__c , &apos;Payment&apos;) )
)</errorConditionFormula>
        <errorMessage>This Service Provider Drawdown record cannot be saved as it will exceed the Service Provider Allowance.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
